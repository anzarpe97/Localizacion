<odoo>
    <data noupdate="1">
        <record id="view_move_form_seniat_readonly" model="ir.ui.view" groups="seniat_audit_compliance.group_seniat_audit"> 
            <field name="name">Account Move Form SENIAT Readonly</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="/" position="attributes">
                    <attribute name="create">0</attribute>
                    <attribute name="edit">0</attribute>
                    <attribute name="delete">0</attribute>
                </xpath>
            </field>
        </record>
        <record id="sale.sale_menu_root" model="ir.ui.menu">
            <field name="groups_id" eval="[
                (4, ref('seniat_audit_compliance.group_seniat_audit')),
                (4, ref('sales_team.group_sale_manager')),
                (4, ref('sales_team.group_sale_salesman_all_leads')),
                (4, ref('sales_team.group_sale_salesman'))
            ]"/>
        </record>
        <record id="sale.menu_sale_quotations" model="ir.ui.menu">
            <field name="groups_id" eval="[
                (4, ref('seniat_audit_compliance.group_seniat_audit')),
                (4, ref('sales_team.group_sale_manager')),
                (4, ref('sales_team.group_sale_salesman_all_leads')),
                (4, ref('sales_team.group_sale_salesman'))
            ]"/>
        </record>
        <record id="sale.res_partner_menu" model="ir.ui.menu">
            <field name="groups_id" eval="[
                (4, ref('seniat_audit_compliance.group_seniat_audit')),
                (4, ref('sales_team.group_sale_manager')),
                (4, ref('sales_team.group_sale_salesman_all_leads')),
                (4, ref('sales_team.group_sale_salesman'))
            ]"/>
        </record>
        <record id="sale.action_quotations" model="ir.actions.act_window">
            <field name="groups_id" eval="[
                (4, ref('seniat_audit_compliance.group_seniat_audit')),
                (4, ref('sales_team.group_sale_manager')),
                (4, ref('sales_team.group_sale_salesman_all_leads')),
                (4, ref('sales_team.group_sale_salesman'))
            ]"/>
        </record>
    </data>
</odoo>